{
  "name": "markdown-named-codeblocks",
  "displayName": "Markdown Named CodeBlocks",
  "description": "Adds named code blocks support to the build-in markdown preview",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.50.0"
  },
  "keywords": [
    "markdown",
    "named-code-blocks",
    "fence",
    "code blocks",
    "named code blocks",
    "named"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "icon": "logo/logox128.png",
  "publisher": "tsutsu3",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tsutsu3/vscode-named-code-blocks"
  },
  "bugs": {
    "url": "https://github.com/tsutsu3/vscode-named-code-blocks/issues"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true,
    "markdown.previewStyles": [
      "named-code-blocks.css"
    ]
  },
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "lint": "eslint .",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch"
  },
  "dependencies": {
    "markdown-it-named-code-blocks": "^1.0.6"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
